@model IEnumerable<Domein.ExtraAttribute>

@{
    ViewBag.Title = "Index";
}

<h2>Attributes</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AttributeType.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AttributeType.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AttributeID }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.AttributeID }) |*@
            @Html.ActionLink("Delete", "Delete", new { id=item.AttributeID })
        </td>
    </tr>
}

</table>
